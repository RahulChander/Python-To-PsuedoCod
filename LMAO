def Load(myfile):
    List = []
    w = open(myfile,'r')
    for line in w:
        w = str(line)
        List.append(w)
    return(List)
OVERALL = Load('PYTHONTEST.py')
FUnctionList = []

def makeIfCaps():
    global OVERALL
    List = OVERALL
    NewList = []
    for i in List:
        w = i.replace(' if ',' IF ')
        NewList.append(w)
    OVERALL = NewList

def makePercent():
    global OVERALL
    List = OVERALL
    NewList = []
    for i in List:
        w = i.replace('%','MOD')
        NewList.append(w)
    OVERALL = NewList

def makeDivision():
    global OVERALL
    List = OVERALL
    NewList = []
    for i in List:
        w = i.replace('//','DIV')
        NewList.append(w)
    OVERALL = NewList

