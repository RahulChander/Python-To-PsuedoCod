        NewList.append(w)
    OVERALL = NewList

def arrows():
    global OVERALL
    List = OVERALL
    NewList = []
    for i in List:
        w = i.replace(' = ',' ‚Üê ')
        NewList.append(w)
    OVERALL = NewList

